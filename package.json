{
  "name": "bookiza",
  "version": "1.0.0-semver-ized",
  "description": "A book baking tool",
  "logo": {
    "file": "./assets/images/bookiza.png"
  },
  "preferGlobal": true,
  "engines": {
    "node": "6.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bookiza/bookiza.git"
  },
  "keywords": [
    "bookiza",
    "book",
    "writing",
    "tool",
    "manuscript",
    "baking",
    "framework",
    "superbooks",
    "CLI"
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/bookiza/bookiza/issues"
  },
  "homepage": "http://bookiza.io",
  "READme": "https://github.com/bookiza/bookiza#readme",
  "author": "Marvin Danig <marvin@bubbl.in> (https://marvindanig.com)",
  "scripts": {
    "commit": "git cz",
    "test": "mocha ./tests/start.tests.js -w",
    "test:once": "mocha ./tests/start.tests.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "postinstall": "node ./scripts/displayLicense.js"
  },
  "bin": {
    "bookiza": "./bin/bin.js",
    "b": "./bin/bin.js"
  },
  "dependencies": {
    "arc-bookiza": "0.0.13",
    "book-length": "1.0.4",
    "chalk": "1.1.3",
    "check-types": "7.0.1",
    "co": "4.6.0",
    "co-prompt": "1.0.0",
    "commander": "2.9.0",
    "dateformat": "1.0.12",
    "fs-promise": "1.0.0",
    "git-bookiza": "0.0.8",
    "is-positive-integer": "1.1.1",
    "jsonfile": "2.4.0",
    "npm-update-module": "1.0.5",
    "os-homedir": "1.0.2",
    "progress": "1.1.8",
    "shelljs": "0.7.5",
    "spawn-npm-install": "1.2.0",
    "superagent": "3.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "1.3.2",
    "mocha": "3.1.2",
    "semantic-release": "^6.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:once"
    }
  }
}
